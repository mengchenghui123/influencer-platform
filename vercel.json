{
    "builds": [
      {
        "src": "influencer_platform/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb" }
      }
    ],
    "routes": [
        {
            "src": "/staticfiles/(.*)",
            "dest": "/staticfiles/$1"
        },
        { "src": "/(.*)", "dest": "influencer_platform/wsgi.py" }
    ]
  }
  